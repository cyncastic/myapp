.center.jumbotron
  .page-header
    %h1 Log in
  = form_for(:session, url: login_path) do |f|
    .form-group
      = f.label :email
      = f.email_field :email, class: 'form-control'
    .form-group
      = f.label :password
      = f.password_field :password, class: 'form-control'
    = f.submit 'Log in', class: 'btn btn-lg btn-primary'